<!DOCTYPE html>
<html>
	<head>
	  <title><%=full_name(yield(:title)) %></title>
	  <%= stylesheet_link_tag    "application", :media => "all" %>
	  <%= javascript_include_tag "application" %>
	  <%= csrf_meta_tags %>
	</head>
	<body>
		<%= render 'layout/header'%>
		<div class="container"	>
			<%= yield %>
			<%= render 'layout/footer'%>
			<%= debug(params) if Rails.env.development?%>
		</div>
	</body> 
</html>
